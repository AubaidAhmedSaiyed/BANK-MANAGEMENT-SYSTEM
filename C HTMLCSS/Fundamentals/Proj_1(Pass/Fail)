#include <stdio.h>



int main() {
    // Write C code here
    printf("Welcome to New Age Mehcanism !\n");
    printf("Let's have a look into your marks of each subject\n");
    printf("......\n");
    float subject_1,subject_2,subject_3;
    printf("Enter of Subject_1:");
    scanf("%f",&subject_1);
    printf("Enter of Subject_2:");
    scanf("%f",&subject_2);
    printf("Enter of Subject_3:");
    scanf("%f",&subject_3);
    float percentage =(subject_1+subject_2+subject_3)/3;
    if (percentage>=40 && subject_1>=33 && subject_2 >=33 && subject_3>=33){
        printf("Congratulations!\n You have passed the academic year 23-24\n Your Result will be delivered to your address");
    }
    else{
        printf("You have failed in our passing criteria 23-24\n kindly contact our administrative office for next steps.");
    }
    printf("Thank You");
    return 0;
}




